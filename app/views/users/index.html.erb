<h2> Viewing <%= pluralize(@users.size, "User Account") %>. </h2>
<ul class="users">
    <% @users.each do |user| %>
        <li>
            <%= link_to user.private_name, user %>
            created an account
            <%= time_ago_in_words(user.created_at) %>
             ago.
        </li>
    <% end %>
</ul>